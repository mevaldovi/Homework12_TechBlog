<div class="blog-card">
  <div class="blog-card-header">
    <!-- Below we are rendering the name of one dish using a Handlebars.js expression -->
    {{blog_title}}
  </div>
  {{blog_description}}
</div>
<div>
  {{!-- loop through each of the blogs and display the individual blog --}}
  {{#each blogs}}
<div class="blog-title-div">
  <h1>{{blog_title}}</h1>
</div>
<br>
<br>
<div class="blog-description-div">
  <h2>{{blog_description}}</h2>
</div>
  {{/each}}
</div>
